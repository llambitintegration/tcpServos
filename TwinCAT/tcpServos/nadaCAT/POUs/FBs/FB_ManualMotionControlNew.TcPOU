<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.12">
  <POU Name="FB_ManualMotionControlNew" Id="{42114213-2c28-4dec-b04c-4be31c666a9a}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK FB_ManualMotionControlNew
VAR_INPUT
	iCmdMova		:	INT := 0;
	iCmdMovr		: 	INT := 0;
	bEnableDrive	:   BOOL;
	bStartDrive		: 	BOOL;
	bRunOnce		:   BOOL := TRUE;
END_VAR
VAR_OUTPUT
	driveStatus	: ST_AxisStatus;
END_VAR
VAR_IN_OUT
	mcBankAxis	: stServoInterface;
END_VAR

VAR
	eState					: E_ServoState;
	demoState				: FB_ManualStateMachine;
	servoPower				: MC_Power;
	servoBuffer				: MC_BufferMode := MC_Aborting;
	servoReset				: MC_Reset;
	servoHome				: MC_Home;
	servoStatus				: MC_ReadStatus;
	servoSetPosition		: MC_SetPosition;
	servoJog				: MC_Jog;
	servoStop				: MC_Stop;
	servoMoveAbsolute		: MC_MoveAbsolute;
	servoMoveRelative		: MC_MoveRelative;
	servoMoveVelocity		: MC_MoveVelocity;
	jogErrorID				: UDINT;
	errorStop: BOOL;
	disabled: BOOL;
	stopping: BOOL;
	discreteMotion: BOOL;
	continuousMotion: BOOL;
	syncMotion: BOOL;
	homing: BOOL;
	constVelocity: BOOL;
	accel: BOOL;
	decel: BOOL;
	status: ST_AxisStatus;
	powerInterface 		: mcInterface;
	resetInterface		: mcInterface;
	homeInterface		: mcInterface;
	jogInterface		: mcInterface;
	stopInterface		: mcInterface;
	moveAbsInterface	: mcInterface;
	moveRelInterface	: mcInterface;
	moveVelInterface	: mcInterface;
	statusInterface		: mcInterface;
	
	// State Machine Variables
	startCycle			: BOOL;
	continueToTop		: BOOL;
	continueToBottom	: BOOL;
	endAtTop			: BOOL;
	endAtBottom			: BOOL;
	moveBottomTrig			: BOOL;
	moveTopTrig				: BOOL;
	
	demoModePermissive	: BOOL;
	demoModeRun			: BOOL;
	bottomComplete		: BOOL;
	topComplete			: BOOL;
	
	servoMoveBottom		: MC_MoveAbsolute;
	servoMoveTop		: MC_MoveAbsolute;
	servoEstop			: MC_Halt;
	
	demoModePermissivesdf: INT;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[// ~~~~ Manual FB Inits ~~~~
// Status
servoStatus(
	Axis:= mcBankAxis.ioAxisRef, 
	Enable:= TRUE, 
	Valid=> , 
	Busy=> statusInterface.bBusy, 
	Error=> statusInterface.bError, 
	ErrorID=> statusInterface.iErrorID, 
	ErrorStop=> errorStop, 
	Disabled=> disabled, 
	Stopping=> stopping, 
	StandStill=> mcBankAxis.bNotMoving, 
	DiscreteMotion=> discreteMotion, 
	ContinuousMotion=> continuousMotion, 
	SynchronizedMotion=> syncMotion, 
	Homing=> homing, 
	ConstantVelocity=> mcBankAxis.bInVelocity, 
	Accelerating=> mcBankAxis.bAccelerating, 
	Decelerating=> mcBankAxis.bDecelerating, 
	Status=> status);

// Enable
servoPower(
	Axis:= mcBankAxis.ioAxisRef, 
	Enable:= mcBankAxis.bDriveEnable, 
	Enable_Positive:=TRUE , 
	Enable_Negative:=TRUE , 
	Override:=100.0 , 
	BufferMode:=servoBuffer , 
	Options:= servoPower.Options, 
	Busy=> powerInterface.bBusy, 
	Active=> powerInterface.bActive, 
	Error=> powerInterface.bError, 
	ErrorID=> powerInterface.iErrorID);
// Init Stop
servoStop(
	Axis:= mcBankAxis.ioAxisRef, 
	Execute:=mcBankAxis.bStopMotion , 
	Deceleration:= 100, 
	Jerk:=100 , 
	Options:= servoStop.Options, 
	Busy=>stopInterface.bBusy , 
	Active=>stopInterface.bActive , 
	CommandAborted=>stopInterface.bCommandAborted , 
	Error=> stopInterface.bError, 
	ErrorID=> stopInterface.iErrorID);

// Init HOM
servoHome(
	Axis:= mcBankAxis.ioAxisRef, 
	Execute:= mcBankAxis.bHome, 
	Position:=0.0 , 
	HomingMode:= MC_DefaultHoming, 
	BufferMode:= servoBuffer, 
	bCalibrationCam:=gvlIO.bHomeSwitch , 
	Busy=>homeInterface.bBusy , 
	Active=> homeInterface.bActive, 
	CommandAborted=>homeInterface.bCommandAborted , 
	Error=>homeInterface.bError , 
	ErrorID=> homeInterface.iErrorID);

// Init JOG
servoJog(
	Axis:= mcBankAxis.ioAxisRef, 
	JogForward:= mcBankAxis.bJogFWD, 
	JogBackwards:= mcBankAxis.bJogRev, 
	Mode:= servoJog.Mode, 
	Position:=5 , 
	Velocity:= 1, 
	Acceleration:= 250, 
	Deceleration:= 250, 
	Jerk:= 5000, 
	Busy=>jogInterface.bBusy , 
	Active=>jogInterface.bActive , 
	CommandAborted=>jogInterface.bCommandAborted , 
	Error=>jogInterface.bError , 
	ErrorID=>jogInterface.iErrorID );


// Init MOVA
servoMoveAbsolute(
	Axis:= mcBankAxis.ioAxisRef, 
	Execute:= (mcBankAxis.bTriggerMove AND (mcBankAxis.iMotionSelect = 1)), 
	Position:= mcBankAxis.rPosition, 
	Velocity:=mcBankAxis.rSpeed , 
	Acceleration:=mcBankAxis.rAccelDecel , 
	Deceleration:= mcBankAxis.rAccelDecel, 
	Jerk:= 500, 
	BufferMode:= servoBuffer, 
	Options:=servoMoveAbsolute.Options , 
	Busy=> moveAbsInterface.bBusy, 
	Active=> moveAbsInterface.bActive, 
	CommandAborted=>moveAbsInterface.bCommandAborted , 
	Error=> moveAbsInterface.bError, 
	ErrorID=> moveAbsInterface.iErrorID);

// Init MovVel
servoMoveVelocity(
	Axis:= mcBankAxis.ioAxisRef, 
	Execute:= (mcBankAxis.bTriggerMove AND (mcBankAxis.iMotionSelect = 3)), 
	Velocity:= mcBankAxis.rSpeed, 
	Acceleration:= mcBankAxis.rAccelDecel, 
	Deceleration:= mcBankAxis.rAccelDecel, 
	Jerk:= 500, 
	Direction:= servoMoveVelocity.Direction, 
	BufferMode:= servoBuffer, 
	Options:= servoMoveVelocity.Options,  
	Busy=> moveVelInterface.bBusy, 
	Active=> moveVelInterface.bActive, 
	CommandAborted=> moveVelInterface.bCommandAborted, 
	Error=>moveVelInterface.bError , 
	ErrorID=> moveVelInterface.iErrorID);
	
// Init Servo Move Top
servoMoveTop(
	Axis:= mcBankAxis.ioAxisRef, 
	Execute:= demoModeRun, 
	Position:= 800, 
	Velocity:= 150, 
	Acceleration:= 1000, 
	Deceleration:= 1000, 
	Jerk:= 1000, 
	BufferMode:= servoBuffer, 
	Options:= servoMoveAbsolute.Options, 
	Done=> topComplete, 
	Busy=> moveAbsInterface.bBusy, 
	Active=> moveAbsInterface.bActive, 
	CommandAborted=> moveAbsInterface.bCommandAborted, 
	Error=> moveAbsInterface.bError, 
	ErrorID=> moveAbsInterface.iErrorID);

// Init Servo Move Bottom
	servoMoveBottom(
	Axis:= mcBankAxis.ioAxisRef, 
	Execute:= demoModeRun, 
	Position:= 800, 
	Velocity:= 150, 
	Acceleration:= 1000, 
	Deceleration:= 1000, 
	Jerk:= 1000, 
	BufferMode:= servoBuffer, 
	Options:= servoMoveAbsolute.Options, 
	Done=> bottomComplete,
	Busy=> moveAbsInterface.bBusy, 
	Active=> moveAbsInterface.bActive, 
	CommandAborted=> moveAbsInterface.bCommandAborted, 
	Error=> moveAbsInterface.bError, 
	ErrorID=> moveAbsInterface.iErrorID);

// Sync Axis
syncAxis();	

// ~~~~~ Demo State Machine ~~~~~
// FB Init
demoState(
	startCycle:= startCycle, 
	continueToTop:= continueToTop, 
	continueToBottom:= continueToBottom, 
	endAtTop:= endAtTop, 
	endAtBottom:= endAtBottom, 
	moveBottom=> moveBottomTrig, 
	moveTop=> moveTopTrig);
	
// Cycle Start
IF demoModePermissive AND demoModeRun THEN
	startCycle := TRUE;
ELSE 
	startCycle := FALSE;
END_IF

// Continue to Top
IF bottomComplete AND demoModeRun THEN
	continueToTop := TRUE;
ELSE
	continueToTop := FALSE;
END_IF


// Continue to Bottom
IF topComplete AND demoModeRun THEN
	continueToBottom := TRUE;
ELSE
	continueToBottom := FALSE;
END_IF

// End at Bottom
IF bottomComplete AND NOT demoModeRun THEN
	endAtBottom := TRUE;
ELSE
	endAtBottom := FALSE;
END_IF

// End at Top
IF topComplete AND demoModeRun THEN
	endAtTop := TRUE;
ELSE
	endAtTop := FALSE;
END_IF]]></ST>
    </Implementation>
