    <Action Name="syncAxis" Id="{b4948e3a-57ad-4537-b4fa-1f1805e05d68}">
      <Implementation>
        <ST><![CDATA[CASE mcBankAxis.ioAxisRef.NcToPlc.AxisState OF
	0 : // Inactive
	
	1 : // Running
		
	3 : // Phase_VeloConst
		mcBankAxis.bInVelocity := TRUE;
	4 : // Phase_AccPos
		mcBankAxis.bAccelerating := TRUE;
	5 : // Phase_AccNeg
		mcBankAxis.bDecelerating := TRUE;
END_CASE

// Feedback

mcBankAxis.rActualPosition := mcBankAxis.ioAxisRef.NcToPlc.ActPos;

mcBankAxis.rActualSpeed := mcBankAxis.ioAxisRef.NcToPlc.ActVelo;

// Motion Status
mcBankAxis.bDriveEnabled:= NOT(disabled);
mcBankAxis.bMoving := NOT(mcBankAxis.bNotMoving AND disabled);
mcBankAxis.bFaulted := statusInterface.bError;

// Drive Control

// Demo Mode
IF mcBankAxis.iMotionSelect = 1 THEN
	demoModePermissive := TRUE;
ELSE
	demoModePermissive := FALSE;
END_IF

IF demoModePermissive AND (mcBankAxis.bDemoModeStart OR demoModeRun) AND NOT mcBankAxis.bDemoModeStop THEN
	demoModeRun := TRUE;
ELSE
	demoModeRun := FALSE;
END_IF]]></ST>
      </Implementation>
    </Action>
